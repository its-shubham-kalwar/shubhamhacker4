#include<stdio.h>
int main()
{
	float marks;
	char grade;
	
	
	printf("Enter marks: \n ");
	scanf("%f", &marks );
	
	if(marks >=85)
	{
		
		grade = 'A' ;
		
	}
	    
	    else if(marks >=70  && marks <=84 )
	    {
	    	grade = 'B' ;
		}
		
		else if (marks >=55  && marks <=69)
		{
			grade = 'C'  ;
		}
		else if (marks >=40  &&  marks<=54 )
		{
			grade = 'D'  ;
		}
		else if (marks >=0 &&  marks<=39)
		{
			grade = 'F' ;
		}
		
		printf("Your grade is  %c", grade);
		
		
		return  0;
}
